{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-decorators-legacy",
    /** 
    * * * * * * * * * * * * * * * * *
    * Convert async ES7 to promise  *
    * * * * * * * * * * * * * * * * *
    * async function test() {
    *   await db.destroy();
    * }
    */
    "kneden",
    /**
    Object.assign({}, obj1, obj2)
    */
    "transform-object-assign"
  ]
}
