{
  "name": "Rocket",
  "version": "2.0.0",
  "description": "This is isomorphic boilerplate with react, redux, tests",
  "license": "MIT",
  "scripts": {
    "p": "set NODE_ENV=production&& node build.production",
    "w": "set NODE_ENV=development&& webpack-dev-server --history-api-fallback --hot --inline --progress --colors",
    "isomorphic": "set NODE_ENV=development&& node isomorphic/babel.server.js --harmony",
    "test-w": "set NODE_ENV=development&& webpack-dev-server --history-api-fallback --inline --progress --colors --tests",
    "_compile": "node_modules/.bin/babel src/ --out-dir node_modules/_temp",
    "_plato": "node_modules/.bin/plato -r -d reports/metrics node_modules/_temp",
    "metrics": "npm run _compile && npm run _plato",
    "count": "node_modules/.bin/sloc --format cli-table --keys total,source,comment src/ reports/",
    "doc": "node_modules/.bin/jsdoc src -r -c ./.jsdoc.conf.json -d documentation/ README.md"
  },
  "devDependencies": {
    "@philholden/redux-swarmlog": "0.2.0",
    "autoprefixer": "6.3.3",
    "babel": "6.5.2",
