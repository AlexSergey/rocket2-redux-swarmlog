    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "3.16.1",
    "expect": "1.14.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jsdoc": "3.4.0",
